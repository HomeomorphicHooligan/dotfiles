# This file stores the configuration of my ZSH shell
#
# For more information: github.com/pablocorbalann/dotfiles/tree/main/zsh

# Export oh my zsh
export ZSH=$HOME/.oh-my-zsh

ZSH_THEME="evan"

# Plugins
source ~/plugins.zsh
# Source OH MY ZSH
source $ZSH/oh-my-zsh.sh
# Alias
source ~/alias.zsh
# Functions
source ~/.functions
